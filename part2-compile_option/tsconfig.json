{
  "include": ["./src/**/*"],
  "exclude": ["./src/testIndex/**/*"],
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    // "lib": ["DOM", "ES2015"]
    "outDir": "./dist",
    // "outFile": "./dist/app.js",
    "allowJs": false,
    "checkJs": false,
    "removeComments": false,
    "noEmit": false,
    "noEmitOnError": false,
    "alwaysStrict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": false
  }
}
